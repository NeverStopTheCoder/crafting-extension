         let y = Math.floor((cursorSlot2 - 1) / 2) * 30 + 40;\n            cursor2.setPosition(x, y);\n        }\n    }\n    // Get Cursor Slot\n    //% block=\"cursor slot\"\n    //%group=cursor\n    //%subcategory=\"2x2 grid\"\n    export function getCursorSlot2(): number {\n        return cursorSlot2;\n    }\n    //%group=items\n    // Place Item in Slot\n    //% block=\"place item $name with image $image at slot $slot\"\n    //% image.shadow=\"image_picker\"\n    //% slot.min=1 slot.max=4\n    //%subcategory=\"2x2 grid\"\n    export function placeItem2(name: string, image: Image, slot: number) {\n        if (open2 == true) {\n        if (!grid2[slot - 1]) {\n            let x = ((slot - 1) % 2) * 30 + 60;\n            let y = Math.floor((slot - 1) / 2) * 30 + 40;\n            let itemSprite = sprites.create(image, SpriteKind.Food);\n            itemSprite.setPosition(x, y);\n            itemSprite.setFlag(SpriteFlag.RelativeToCamera, true)\n            grid2[slot - 1] = name;\n        }\n    }\n    }\n    //%group=slots\n    // Check if Slot Contains Item\n    //% block=\"slot $slot contains $name\"\n    //% slot.min=1 slot.max=4\n    //%subcategory=\"2x2 grid\"\n    export function checkItemInSlot2(slot: number, name: string): boolean {\n        return grid2[slot - 1] == name;\n    }\n\n    //%group=items\n    // Get Crafted Item\n    //% block=\"crafted item name\"\n    //%subcategory=\"2x2 grid\"\n    export function getCraftedItem2(): string {\n        return craftedItem2;\n    }\n    //%group=items\n    // Craft Item (Clear Grid, Destroy Items, & Place Crafted Item in Center)\n    //% block=\"craft item $craftedItem with image $craftedImage\"\n    //% craftedImage.shadow=\"image_picker\"\n    //%subcategory=\"2x2 grid\"\n    export function craftItem3(craftedItem: string, craftedImage: Image) {\n        if (open2 == true)\n        // Clear the grid array\n        for (let i = 0; i < 4; i++) {\n            grid2[i] = null;\n        }\n        // Destroy all 