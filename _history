 slot number (0-8 for a 3x3 grid)\n    //%block=\"is Cursor at slot $slotIndex (3x4)\"\n    //%subcategory=\"3x4 grid\"\n    export function isCursorAtSlot3(slotIndex: number): boolean {\n        return cursorSlot3 == slotIndex;\n    }\n    // Function to check if cursor is on any slot from the array\n    //%block=\"is Cursor on any of theses slots $validSlots (3x4)\"\n    //%group=cursor\n    //%subcategory=\"3x4 grid\"\n    export function isCursorOnAnySlot3(validSlots: number[]): boolean {\n        for (let i = 0; i < validSlots.length; i++) {\n            if (cursorSlot3 == validSlots[i]) {\n                return true;\n            }\n        }\n        return false;\n    }\n    //%block=\"is Slot Occupied $slot (3x4)\"\n    //%group=slots\n    //%subcategory=\"3x4 grid\"\n    export function isSlotOccupied3(slot: number): boolean {\n        return grid3[slot - 1] != null\n    }\n    //%group=slots\n    //% block=\"all other slots are occupied (except used slots: $usedSlots) (3x4)\"\n    //%subcategory=\"3x4 grid\"\n    export function allOtherSlotsOccupiedExcept3(usedSlots: number[]): boolean {\n        for (let i = 0; i < 12; i++) {  // Assuming a 9-slot grid\n            if (usedSlots.indexOf(i + 1) == -1) {\n                // If the slot is NOT in the usedSlots array and it is empty, return false\n                if (grid3[i] == null) {\n                    return false;\n                }\n            } else {\n                // If the slot IS in the usedSlots array and it is NOT empty, return false\n                if (grid3[i] != null) {\n                    return false;\n                }\n            }\n        }\n        return true; // If all conditions are met, return true\n    }\n\n}","pxt.json":"{\n    \"name\": \"Crafting extension\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"device\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"assets.json\",\n       