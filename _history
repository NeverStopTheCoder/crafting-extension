\n    }\n    // Get Crafted Image\n    //% block=\"crafted item image\"\n    //% blockId=get_crafted_image\n    //%subcategory=\"2x2 grid\"\n    export function getCraftedImage2(): Image {\n    return craftedSprite2.image;\n}\n\n//% block=\"slot $slot is empty\"\n//% slot.min=1 slot.max=9\n//%subcategory=\"2x2 grid\"\nexport function isSlotEmpty2(slot: number): boolean {\n    return grid2[slot - 1] = null;\n}\n\n//% block=\"all other slots are empty (except used slots: $usedSlots)\"\n//%subcategory=\"2x2 grid\"\nexport function allOtherSlotsEmpty2(usedSlots: number[]): boolean {\n    for (let i = 0; i < 4; i++) {\n        // If the slot is NOT in the usedSlots list AND it is NOT empty, re\n        if (usedSlots.indexOf(i + 1) == -1 && grid2[i] != null) 1\n        return false;\n    }\n    return true;\n}\n    //%block=\"exit crafting table\"\n    //%subcategory=\"2x2 grid\"\n    export function exitCraftingTable2() {\n        // Destroy all grid slots\n        for (let sprite of gridSprites2) {\n            sprite.destroy();\n        }\n        gridSprites2 = [];\n        // Destroy the cursor\n        if (cursor2) {\n            cursor2.destroy();\nfor (let sprite of sprites.allOfKind(SpriteKind.Food)) {\n                sprite.destroy();\n                // Clear the crafting grid data\n                grid2 = [null, null, null, null,];\n}\n}\n    }\n    //%group=items\n    // Remove Item from Slot\n    //% block=\"remove item at slot $slot\"\n    //% slot.min=1 slot.max=9\n    //%subcategory=\"2x2 grid\"\n    export function removeItem2(slot: number) {\n        if (grid2[slot - 1]) {\n            // Find the sprite in the gridSprites array at the corresponding po\n            let x = ((slot - 1) % 3) * 30 + 50;\n            let y = Math.floor((slot - 1) / 3) * 30 + 30;\n            for (let\n                sprite of sprites.allOfKind(SpriteKind.Food)) {\n                if (sprite.x && sprite.y == y) {\n                    sprite.destroy();\n                    