 () {\n            if (cursorSlot3 % 4 != 0) {\n                cursorSlot3++;\n                updateCursor2();\n            }\n        });\n\n        controller.up.onEvent(ControllerButtonEvent.Pressed, function () {\n            if (cursorSlot3 > 4) {\n                cursorSlot3 -= 4;\n                updateCursor2();\n            }\n        });\n\n        controller.down.onEvent(ControllerButtonEvent.Pressed, function () {\n            if (cursorSlot3 < 9) {\n                cursorSlot3 += 4;\n                updateCursor2();\n            }\n        });\n\n        // Update Cursor Position\n        function updateCursor2() {\n            let x = ((cursorSlot3 - 1) % 4) * 30 + 40;\n            let y = Math.floor((cursorSlot3 - 1) / 4) * 30 + 30;\n            cursor3.setPosition(x, y);\n        }\n    }\n    // Get Cursor Slot\n    //% block=\"cursor slot(3x4)\"\n    //%group=cursor\n    //%subcategory=\"3x4 grid\"\n    export function getCursorSlot3(): number {\n        return cursorSlot3;\n    }\n    //%group=items\n    // Place Item in Slot\n    //% block=\"place item $name with image $image at slot $slot (3x4)\"\n    //% image.shadow=\"image_picker\"\n    //% slot.min=1 slot.max=12\n    //%subcategory=\"3x4 grid\"\n    export function placeItem3(name: string, image: Image, slot: number) {\n        if (open3 == true) {\n        if (!grid3[slot - 1]) {\n            let x = ((slot - 1) % 4) * 30 + 40;\n            let y = Math.floor((slot - 1) / 4) * 30 + 30;\n            let itemSprite = sprites.create(image, SpriteKind.Food);\n            itemSprite.setPosition(x, y);\n            itemSprite.setFlag(SpriteFlag.RelativeToCamera, true)\n            grid3[slot - 1] = name;\n        }\n        }\n    }\n    //%group=slots\n    // Check if Slot Contains Item\n    //% block=\"slot $slot contains $name (3x4)\"\n    //% slot.min=1 slot.max=12\n    //%subcategory=\"3x4 grid\"\n    export function checkItemInSlot3(slot: number, name: string): boolean {\n        return g